{
  "name": "transcode-video",
  "version": "1.0.0",
  "description": "This Lambda function will create a job to transcode videos uploaded to an S3 bucket",
  "main": "index.js",
  "scripts": {
    "test": "run-local-lambda --file index.js --event tests/event.json",
    "deploy": "aws lambda update-function-code --function-name arn:aws:lambda:us-east-1:921598642698:function:transcode-video --zip-file fileb://Lambda-Deployment.zip",
    "predeploy": "7z a -r -x!*.zip -x!*.json -x!*.log Lambda-Deployment.zip *"
  },
  "dependencies": {
    "aws-sdk": "latest"
  },
  "author": "Shahid ul Islam",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "run-local-lambda": "^1.1.1"
  }
}
